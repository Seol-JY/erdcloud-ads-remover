{
  "name": "Erd Cloud Ads Remover",
  "description": "Removes Erd Cloud Ads.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["tabs", "activeTab", "scripting", "webNavigation"],
  "host_permissions": ["*://*.erdcloud.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.erdcloud.com/*"],
      "js": ["script.js"]
    }
  ],
  "author": "wlsdud5654@gmail.com"
}
